{
  "name": "zip",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "react": "^18.0.0-alpha-bc9bb87c2-20210917",
    "react-dom": "^18.0.0-alpha-bc9bb87c2-20210917",
    "react-paginate": "^7.1.3",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2",
    "webpack": "^5.53.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "ssr": "concurrently \"npm run bundleClient\" \"npm run buildServer\" \"npm run runServer\"",
    "buildServer": "NODE_ENV=development webpack",
    "runServer": "cross-env NODE_ENV=development nodemon -- dist/bundle.js",
    "bundleClient": "cross-env NODE_ENV=development node scripts/build.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "import-normalize"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@types/react-paginate": "^7.1.1",
    "prettier": "^2.4.1",
    "quicktype": "^15.0.260",
    "react-app-rewired": "^2.1.8",
    "react-loading-skeleton": "^2.2.0",
    "webpack-cli": "^4.8.0"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  }
}
